<!-- Fetches Information of selected product in Review Pop up -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>

<script>

    // Last Order Review Carousel

    $('.js--review-popup').owlCarousel({
        items: 5,
        slideBy: 4,
        loop: false,
        margin: 6,
        nav: true,
        dots: false
    });

    const pageContext = $('meta[name=context-path]').attr('content');

    $('.js--not-reviewed-product').click(function(e) {
        e.preventDefault();
        $('#js--review-writing').val('');
        $('.js--not-reviewed-product').removeClass('active');
        $('.ratings').removeClass('ion-ios-star').addClass('ion-ios-star-outline');
        $(this).addClass('active');
        const productId = $(this).attr('data-id');
        $('#js--review-submit').data('id', productId);
        $('#js--home-rating').data('id', productId);
        const productName = $(this).attr('data-name');
        const productImage = $(this).attr('data-image');
        const productAuthor = $(this).attr('data-author');
        $('#js--product-img').attr('src', 'https://ds.rokomari.store/rokomari110/ProductNew20190903/130X186/'+productImage);
        $('#js--product-name').html(productName);
        $('#js--product-author').html(productAuthor);
    });

    // login with fb and google
    function callFacebookLogin() {
        window.location.href = "https://www.rokomari.com/auth/oauth2/authorization/facebook";
    }

    function callGoogleLogin() {
        window.location.href = "https://www.rokomari.com/auth/oauth2/authorization/google";
    }

</script>



    
